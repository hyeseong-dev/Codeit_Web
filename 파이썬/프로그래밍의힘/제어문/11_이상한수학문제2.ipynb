    "# 함수로 표현하기/n",
    "def sum_(num):/n",
    "    i = 1/n",
    "    result = 0/n",
    "    for i in range(1,num):/n",
    "        if i%2 == 0 or i%3 == 0:/n",
    "            result += i/n",
    "    return result/n",
    "/n",
    "sum_(1000)"
   ]
